    <!-- Front End MetaData -->
    <em:name>GFS</em:name>
    <em:description>Helps find interesting files on the web.</em:description>
    <em:creator>francoijs@github.com</em:creator>
    <em:homepageURL>https://github.com/francoijs/gfs/</em:homepageURL>
  </Description>      
</RDF>
